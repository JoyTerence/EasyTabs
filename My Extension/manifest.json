{
  "manifest_version": 2,

  "name": "EasyTabs",
  "description": "This extension helps to navigate when tabs are many! ",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
